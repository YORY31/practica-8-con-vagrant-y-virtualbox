Vagrant.configure("2") do |config|
    config.vm.box = "ubuntu/bionic64" # O la caja específica que estás usando
    config.vm.network "private_network", ip: "192.168.56.10"
    config.vm.network "forwarded_port", guest: 8080, host: 8080 # Esta línea es la que necesitas
    config.vm.synced_folder ".", "/var/www/html" # Sincroniza el directorio actual con /var/www/html
    config.vm.provider "virtualbox" do |vb|
      vb.memory = "512"
      vb.cpus = 1
    end
end
